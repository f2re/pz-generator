{
  "project_name": "PZ Generator",
  "version": "1.0.0",
  "experimental": {
    "enableAgents": true,
    "agentSettings": {
      "enabled": true,
      "orchestratorSettings": {
        "enabled": true,
        "autoDelegate": true
      },
      "developerSettings": {
        "enabled": true,
        "pythonVersion": "3.11",
        "formatter": "black",
        "linter": "ruff"
      },
      "testerSettings": {
        "enabled": true,
        "framework": "pytest",
        "minCoverage": 80
      }
    }
  },
  "codeExecution": {
    "enabled": true,
    "timeout": 300
  },
  "fileSearch": {
    "enabled": true,
    "excludePatterns": [
      "*.pyc",
      "__pycache__",
      ".git",
      "venv",
      "*.xlsx",
      "*.csv"
    ]
  },
  "api_settings": {
    "provider": "google",
    "base_url": null,
    "timeout": 60
  },
  "paths": {
    "prompts": ".gemini/prompts",
    "workflows": ".gemini/workflows",
    "agents": ".gemini/agents",
    "output": "output",
    "logs": "logs"
  },
  "defaults": {
    "model": "gemini-2.0-flash-exp",
    "temperature": 0.2,
    "max_tokens": 8192
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  }
}
